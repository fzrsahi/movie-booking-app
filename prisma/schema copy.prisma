// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Movie {
  uuid        Int    @id @map("_id")
  title       String
  description String
  releaseDate String
  ageRating   String
  price       String
  poster      String
}
