generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String
  email     String   @unique
  birth     String
  hash      String
  age       Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Movie {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  price       Int
  releaseDate String
  ageRating   Int
  poster      String
}
